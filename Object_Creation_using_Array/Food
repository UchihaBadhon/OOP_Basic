
package org.example;


public class Food {
    
    int age;
    String name;
    
    Food( int age, String name ){
        this.age = age;
        this.name = name;
    }
    
    void Identity(){
        System.out.println("Name is "+name);
        System.out.println("Age is "+age);
    }
    void food(){
        System.out.println("I am "+name);
    }
    
    
}
